class Machine {
	public void start() {
		System.out.println("Starting machine...");
	}
}


public class UdemyAnonymous {

	public static void main(String[] args) {
		
		Machine machine1 = new Machine(){
			@Override
			public void start() {
				System.out.println("Camera stopping...");
			}
		};

		machine1.start();
	}

}
